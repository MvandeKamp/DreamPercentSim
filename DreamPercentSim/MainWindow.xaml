<Window x:Class="DreamPercentSim.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DreamPercentSim"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="GetTrys" HorizontalAlignment="Left" Height="61" Margin="10,88,0,0" VerticalAlignment="Top" Width="187" Click="Button_Click"/>
        <Button x:Name="start" Content="Start" HorizontalAlignment="Left" Height="61" Margin="10,22,0,0" VerticalAlignment="Top" Width="187" Click="StarButton_Click"/>
        <ScrollViewer HorizontalAlignment="Left" Height="387" Margin="202,22,0,0" VerticalAlignment="Top" Width="580">
            <TextBlock x:Name="console" TextWrapping="Wrap" Text="Simulator ready"/>
        </ScrollViewer>

    </Grid>
</Window>
